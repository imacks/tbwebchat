{
    "name": "Enable Taobao Webchat",
    "version": "0.1.0.0",
    "manifest_version": 2,
    "description": "Make Taobao use web Wangwang chat",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png",
        "300": "icons/icon300.png"
    },
    "content_scripts": [
        {
            "matches": ["https://item.taobao.com/*"],
            "js": ["js/webwangwang.js"]
        }
    ]
}
